   "source": [
    "#gold.weather_metrics: grouped by weather desc -> metrics:\n",
    "# >total(avg, min, max, sum)\n",
    "# >fare(avg, min, max, sum)\n",
    "# >tips(avg, min, max, sum) \n",
    "# >duration (avg, min, max)\n",
    "# >weather(avg rain, total rain, avg temp, avg wind)\n"
    "# >distance(min, max, avg)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 0,
   "metadata": {
    "application/vnd.databricks.v1+cell": {
     "cellMetadata": {
      "byteLimit": 2048000,
      "rowLimit": 10000
     },
     "inputWidgets": {},
     "nuid": "77bf1530-0b31-45d0-b31a-3acb71a44a60",
     "showTitle": false,
     "tableResultSettingsMap": {},
     "title": ""
    }
   },
   "outputs": [],
   "source": [
    "from pyspark.sql.functions import col, to_date, avg, count, sum, max, min, date_trunc, dayofweek\n",
    "s_main_df = spark.table('`nyc_taxis_weather`.silver_nyc_taxis.s_nyc_taxis_weather_oct_dec').select(\"w_desc\", \"total_amount\", \"fare_amount\", \"tip_amount\", \"trip_duration\", \"trip_distance\", \"prcp\", \"temp\", \"wspd\").drop(col(\"_rescued_data\"))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 0,
   "metadata": {
    "application/vnd.databricks.v1+cell": {
     "cellMetadata": {
      "byteLimit": 2048000,
      "rowLimit": 10000
     },
     "inputWidgets": {},
     "nuid": "78a4d39d-7e0a-4042-b9c3-4fd3d35cc14c",
     "showTitle": false,
     "tableResultSettingsMap": {},
     "title": ""
    }
   },
   "outputs": [],
   "source": [
    "g_weather_metrics = s_main_df.groupBy(\"w_desc\").agg(count(\"*\").alias(\"count_trips\"),\n",
    "                         avg(col(\"total_amount\")).alias(\"avg_total\"),\n",
    "                         min(col(\"total_amount\")).alias(\"min_total\"),\n",
    "                         max(col(\"total_amount\")).alias(\"max_total\"),\n",
    "                         sum(col(\"total_amount\")).alias(\"total_weather_sum\"),\n",
    "                         avg(col(\"fare_amount\")).alias(\"avg_fare\"),\n",
    "                         min(col(\"fare_amount\")).alias(\"min_fare\"),\n",
    "                         max(col(\"fare_amount\")).alias(\"max_fare\"),\n",
    "                         sum(col(\"fare_amount\")).alias(\"fare_weather_sum\"),\n",
    "                         avg(col(\"tip_amount\")).alias(\"avg_tip\"),\n",
    "                         min(col(\"tip_amount\")).alias(\"min_tip\"),\n",
    "                         max(col(\"tip_amount\")).alias(\"max_tip\"),\n",
    "                         sum(col(\"tip_amount\")).alias(\"tip_weather_sum\"),\n",
    "                         avg(col(\"trip_duration\")).alias(\"avg_duration\"),\n",
    "                         min(col(\"trip_duration\")).alias(\"min_duration\"),\n",
    "                         max(col(\"trip_duration\")).alias(\"max_duration\"),\n",
    "                         avg(col(\"trip_distance\")).alias(\"avg_distance\"),\n",
    "                         min(col(\"trip_distance\")).alias(\"min_distance\"),\n",
    "                         max(col(\"trip_distance\")).alias(\"max_distance\"),\n",
    "                         avg(col(\"prcp\")).alias(\"avg_rain\"),\n",
    "                         sum(col(\"prcp\")).alias(\"total_rain\"),\n",
    "                         avg(col(\"temp\")).alias(\"avg_temp\"),\n",
    "                         avg(col(\"wspd\")).alias(\"avg_wind\"),\n",
    "                         (sum(col(\"total_amount\"))/sum(col(\"trip_duration\"))).alias(\"profitability\")).orderBy(\"count_trips\", ascending=False)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 0,
   "metadata": {
    "application/vnd.databricks.v1+cell": {
