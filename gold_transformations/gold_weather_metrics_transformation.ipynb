   "source": [
    "g_weather_metrics.write.option(\"mergeSchema\", \"true\").option(\"overwriteSchema\", \"true\").mode(\"overwrite\").saveAsTable(\"`nyc_taxis_weather`.gold_nyc_taxis.g_weather_metrics\")"
   ]
  }
